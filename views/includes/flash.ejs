<% if (success && success.length > 0) { %>
    <% success.forEach(msg => { %>
      <div class="alert alert-success alert-dismissible fade show shadow-sm rounded-3 px-4 py-3" role="alert">
        <%= msg %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>
    <% }) %>
  <% } %>


  <% if (registration && registration.length > 0) { %>
    <% registration.forEach(msg => { %>
      <div class="alert alert-success alert-dismissible fade show shadow-sm rounded-3 px-4 py-3" role="alert">
        <%= msg %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>
    <% }) %>
  <% } %>


  